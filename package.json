  "version": "0.0.1",
  "description": "Confederation College Discord Roles Bot",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "ts-node src/index.ts",
    "build": "tsc"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testRegex": "(/__tests__/).*(test|spec)\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KristianFJones/Discord-College-Roles-Bot.git"
  },
  "keywords": [
